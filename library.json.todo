{
    "name": "OpenStreetMap-esp32-bb",
    "version": "1.0.6",
    "description": "This library provides a OpenStreetMap (OSM) map fetching and caching system for ESP32-based devices.",
    "keywords": [
        "esp32",
        "gps",
        "location",
        "geolocation",
        "tracking",
        "map",
        "tiles",
        "tile-caching",
        "openstreetmap",
        "osm",
        "bb_spi_lcd",
        "pngdec"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/CelliesProjects/OpenStreetMap-esp32-bb"
    },
    "authors": [
        {
            "name": "CelliesProjects"
        }
    ],
    "frameworks": [
        "arduino"
    ],
    "platforms": [
        "espressif32"
    ],
    "dependencies": [
        {
            "name": "LovyanGFX",
            "version": "https://github.com/lovyan03/LovyanGFX.git#1.2.7"
        },
        {
            "name": "PNGdec",
            "version": "https://github.com/bitbank2/PNGdec.git#28b221d"
        }
    ],
    "build": {
        "flags": [
            "-std=gnu++17"
        ]
    },
    "license": "MIT",
    "include": [
        "src/OpenStreetMap-esp32.hpp"
    ]
}
